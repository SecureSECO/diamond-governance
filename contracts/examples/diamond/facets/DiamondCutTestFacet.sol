import { IDiamondCut } from "../../../additional-contracts/IDiamondCut.sol";
import { LibDiamond } from "../../../libraries/LibDiamond.sol";

// Remember to add the loupe functions from DiamondLoupeFacet to the diamond.
// The loupe functions are required by the EIP2535 Diamonds standard

contract DiamondCutTestFacet is IDiamondCut {
    function diamondCut(FacetCut[] calldata _diamondCut) external virtual override {
        LibDiamond.diamondCut(_diamondCut);
    }
}